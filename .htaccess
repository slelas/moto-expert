RewriteRule ^rabljeno/[^\.]*?([^/\.]*)/$ rabljeno.php?fname=$1 [L,QSA]
RewriteRule ^rabljeno/[^\.]*?([^/\.]*)$ "$0/" [R=301,L,QSA]

